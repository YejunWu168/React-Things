{
  "parser": "babel-eslint",
  "rules": {
    "no-unused-vars": 0,
    "max-len": [
      1,
      120,
      2,
      {
        "ignoreComments": true
      }
    ],
    "react/prop-types": [
      2
    ],
    "no-console": 1,
    "no-undef": 0
  },
  "extends": [
    "plugin:react/recommended"
  ]
}